prometheus:
  url: "http://localhost:8090/plugin/Prometheus"
  plugin_type: "micro-app"
  reverseDest: "http://localhost:9090"
http:
  addr: "0.0.0.0:8090"
log:
  level: debug
  driver: stdout    #可选stdout和file。stdout：输出到终端控制台；file：输出到path下的指定文件。
  path: ./log/plugin_prometheus.log
  max_file: 1
  max_size: 10485760
mysql:
  host: localhost
  port: 3306
  user: ''
  password: ''
  database: PluginPrometheus